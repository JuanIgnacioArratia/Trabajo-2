@article{ochycurielCriticaPoscolonialDesde2007,
  title = {Cr{\'i}tica Poscolonial Desde Las Pr{\'a}cticas Pol{\'i}ticas Del Feminismo Antirracista},
  author = {{Ochy Curiel} and Curiel, Ochy and {Ochy Curiel}},
  year = {2007},
  month = jan,
  journal = {N{\'o}madas},
  number = {26},
  pages = {92--101},
  doi = {10.2307/j.ctv253f4j3.10},
  abstract = {Este articulo senala que la teoria poscolonial hecha desde la academia conlleva una posicion elitista y androcentrica. La autora muestra que las practicas y luchas del movimiento feminista, tanto en los Estados Unidos como en America Latina, han generado una forma de teorizar lo poscolonial que con frecuencia es ignorada por la academia. Trazando un recorrido que va desde los movimientos feministas negros en los Estados Unidos, pasando por el feminismo chicano, el feminismo afrolatino y el feminismo indigena, la autora muestra que la teoria poscolonial se beneficiaria mucho de los grandes aportes que estos movimientos politicos han hecho al pensamiento sobre la dominacion colonial},
